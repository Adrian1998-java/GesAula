<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane fx:id="view" prefHeight="320.0" prefWidth="400.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/15.0.1">
   <top>
      <ToolBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <items>
          <Button fx:id="nuevoButton" mnemonicParsing="false" onAction="#onNuevo" text="Nuevo" />
            <TextField />
            <Button fx:id="guardarButton" defaultButton="true" mnemonicParsing="false" onAction="#onGuardar" text="Guardar" />
        </items>
      </ToolBar>
   </top>
   <center>
      <TabPane prefHeight="200.0" prefWidth="200.0" side="LEFT" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
        <tabs>
          <Tab fx:id="grupoTab" text="Grupo" />
          <Tab fx:id="alumnosTab" text="Alumnos" />
        </tabs>
      </TabPane>
   </center>
</BorderPane>
